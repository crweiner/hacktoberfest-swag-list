def armstrong():
    sum=0
    num=int(input('Enter the Number : '))
    temp = num
    while temp > 0:
       digit = temp % 10
       sum += digit ** 3
       temp //= 10
    if (num==sum):
        print(num,'is an Armstrong number')
    else:
        print(num,'is not an Armstrong number')
        
armstrong()
