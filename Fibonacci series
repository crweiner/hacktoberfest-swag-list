#include<stdio.h>
int main()
{
    int n1=0,n2=1,i,next,no;
printf("Enter the number of elements:");
scanf("%d", &no);
printf("\n %d %d", n1,n2);
for (int i = 0; i < no; i++)
{
    if(i<=1)
    {
        next=i;
    }
    else{
        next=n1+n2;
        n1=n2;
        n2=next;
    }
    printf("%d",next);
}

    return 0;
}
